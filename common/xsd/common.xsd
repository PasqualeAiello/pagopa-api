<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:tns="http://pagopa-api.pagopa.gov.it/common/xsd/common.xsd"
            elementFormDefault="qualified"
            targetNamespace="http://pagopa-api.pagopa.gov.it/common/xsd/common.xsd">

    <xsd:simpleType name="stText35">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="35"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="intestazionePPT">
        <xsd:sequence>
            <xsd:element name="identificativoIntermediarioPA" type="tns:stText35"/>
            <xsd:element name="identificativoStazioneIntermediarioPA" type="tns:stText35"/>
            <xsd:element name="identificativoDominio" type="tns:stText35"/>
            <xsd:element name="identificativoUnivocoVersamento" type="tns:stText35"/>
            <xsd:element name="codiceContestoPagamento" type="tns:stText35"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="stISODate">
        <xsd:restriction base="xsd:date"/>
    </xsd:simpleType>

    <xsd:simpleType name="stISODateTime">
        <xsd:restriction base="xsd:dateTime"/>
    </xsd:simpleType>

    <xsd:simpleType name="stText16">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="16"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stText25">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="25"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stCodiceIdentificativoUnivoco">
        <xsd:annotation>
            <xsd:documentation>Campo alfanumerico che può contenere il codice fiscale o, in alternativa, la partita IVA
                del soggetto.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="2"/>
            <xsd:maxLength value="16"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stText70">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="70"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stText140">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="140"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stAutenticazioneSoggetto">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4"/>
            <xsd:enumeration value="CNS"/><!-- CIE/CNS -->
            <xsd:enumeration value="USR"/><!-- Userid e passaword -->
            <xsd:enumeration value="OTH"/><!-- Altro -->
            <xsd:enumeration value="N/A"/><!-- Non applicabile -->
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stCodiceEsitoPagamento">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]"/>
            <xsd:enumeration value="0"/><!-- Pagamento eseguito -->
            <xsd:enumeration value="1"/><!-- Pagamento non eseguito -->
            <xsd:enumeration value="2"/><!-- Pagamento parzialmente eseguito -->
            <xsd:enumeration value="3"/><!-- Decorrenza termini -->
            <xsd:enumeration value="4"/><!-- Decorrenza termini parziale -->
        </xsd:restriction>
    </xsd:simpleType>


    <xsd:simpleType name="stImportoDiversoDaZero">
        <xsd:restriction base="tns:stImporto">
            <xsd:minInclusive value="0.01"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stNazioneProvincia">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{2}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stTipoVersamento">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BBT"/> <!-- Bonifico Bancario di Tesoreria -->
            <xsd:enumeration value="BP"/> <!--- Bollettino Postale -->
            <xsd:enumeration value="AD"/> <!--- Addebito Diretto -->
            <xsd:enumeration value="CP"/> <!--- Carta di Pagamento -->
            <xsd:enumeration value="PO"/> <!--- Pagamento attivato presso PSP -->
            <xsd:enumeration value="OBEP"/> <!--- On-line Banking E-Payment -->
            <xsd:enumeration value="OTH"/> <!-- Altre forme di versamento -->
            <xsd:enumeration value="JIF"/>  <!-- Bancomat Pay  -->
            <xsd:enumeration value="MYBK"/>  <!-- MyBank Seller Bank -->
            <xsd:maxLength value="4"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stDatiSpecificiRiscossione">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0129]/\S{3,138}"/>
        </xsd:restriction>
    </xsd:simpleType>


    <xsd:simpleType name="stFirmaRicevuta">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="0"/><!-- Firma non richiesta -->
            <xsd:length value="1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stTipoIdentificativoUnivoco">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="G"/><!-- Persona Giuridica -->
            <xsd:enumeration value="A"/><!-- Codice ABI -->
            <xsd:enumeration value="B"/><!-- Codice BIC -->
            <xsd:length value="1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stTipoIdentificativoUnivocoPersFG">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="F"/><!-- Persona Fisica -->
            <xsd:enumeration value="G"/><!-- Persona Giuridica -->
            <xsd:length value="1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stTipoIdentificativoUnivocoPersG">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="G"/><!-- Persona Giuridica -->
            <xsd:length value="1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stBICIdentifier">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]{6}[A-Z2-9][A-NP-Z0-9]([A-Z0-9]{3})?"/>
        </xsd:restriction>
    </xsd:simpleType>


    <xsd:simpleType name="stTipoBollo">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="01"/><!-- Imposta di bollo -->
            <xsd:length value="2"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stTipoAllegatoRicevuta">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ES"/><!-- Esito originario pagamento (come ricevuto da PSP) -->
            <xsd:enumeration value="BD"/><!-- Marca da bollo digitale  -->
            <xsd:length value="2"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="stBase64">
        <xsd:restriction base="xsd:base64Binary">
        </xsd:restriction>
    </xsd:simpleType>


    <!-- Tabella T2 Sigla delle province italiane, Agenzia delle Entrate  -->
    <xsd:simpleType name="stSiglaProvinceAde">
        <xsd:annotation>
            <xsd:documentation>Tabella T2 Sigla delle province italiane, Agenzia delle Entrate --NON PIU UTILIZZATA
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="AG"/> <!-- Agrigento  -->
            <xsd:enumeration value="AL"/> <!-- Alessandria  -->
            <xsd:enumeration value="AN"/> <!-- Ancona  -->
            <xsd:enumeration value="AO"/> <!-- Aosta  -->
            <xsd:enumeration value="AP"/> <!-- Ascoli Piceno  -->
            <xsd:enumeration value="AQ"/> <!-- L'Aquila  -->
            <xsd:enumeration value="AR"/> <!-- Arezzo  -->
            <xsd:enumeration value="AT"/> <!-- Asti  -->
            <xsd:enumeration value="AV"/> <!-- Avellino  -->
            <xsd:enumeration value="BA"/> <!-- Bari  -->
            <xsd:enumeration value="BG"/> <!-- Bergamo  -->
            <xsd:enumeration value="BI"/> <!-- Biella  -->
            <xsd:enumeration value="BL"/> <!-- Belluno  -->
            <xsd:enumeration value="BN"/> <!-- Benevento  -->
            <xsd:enumeration value="BO"/> <!-- Bologna  -->
            <xsd:enumeration value="BR"/> <!-- Brindisi  -->
            <xsd:enumeration value="BS"/> <!-- Brescia  -->
            <xsd:enumeration value="BT"/> <!-- Barletta-Andria-Trani  -->
            <xsd:enumeration value="BZ"/> <!-- Bolzano  -->
            <xsd:enumeration value="CA"/> <!-- Cagliari  -->
            <xsd:enumeration value="CB"/> <!-- Campobasso  -->
            <xsd:enumeration value="CE"/> <!-- Caserta  -->
            <xsd:enumeration value="CH"/> <!-- Chieti  -->
            <xsd:enumeration value="CI"/> <!-- Carbonia Iglesias  -->
            <xsd:enumeration value="CL"/> <!-- Caltanisetta  -->
            <xsd:enumeration value="CN"/> <!-- Cuneo  -->
            <xsd:enumeration value="CO"/> <!-- Como  -->
            <xsd:enumeration value="CR"/> <!-- Cremona  -->
            <xsd:enumeration value="CS"/> <!-- Cosenza  -->
            <xsd:enumeration value="CT"/> <!-- Catania  -->
            <xsd:enumeration value="CZ"/> <!-- Catanzaro  -->
            <xsd:enumeration value="EN"/> <!-- Enna  -->
            <xsd:enumeration value="FC"/> <!-- Forlì-Cesena  -->
            <xsd:enumeration value="FE"/> <!-- Ferrara  -->
            <xsd:enumeration value="FG"/> <!-- Foggia  -->
            <xsd:enumeration value="FI"/> <!-- Firenze  -->
            <xsd:enumeration value="FM"/> <!-- Fermo  -->
            <xsd:enumeration value="FR"/> <!-- Frosinone  -->
            <xsd:enumeration value="GE"/> <!-- Genova  -->
            <xsd:enumeration value="GO"/> <!-- Gorizia  -->
            <xsd:enumeration value="GR"/> <!-- Gorizia  -->
            <xsd:enumeration value="IM"/> <!-- Imperia  -->
            <xsd:enumeration value="IS"/> <!-- Isernia  -->
            <xsd:enumeration value="KR"/> <!-- Crotone  -->
            <xsd:enumeration value="LC"/> <!-- Lecco  -->
            <xsd:enumeration value="LE"/> <!-- Lecce  -->
            <xsd:enumeration value="LI"/> <!-- Livorno  -->
            <xsd:enumeration value="LO"/> <!-- Lodi  -->
            <xsd:enumeration value="LT"/> <!-- Latina  -->
            <xsd:enumeration value="LU"/> <!-- Lucca  -->
            <xsd:enumeration value="MB"/> <!-- Monza-Brianza  -->
            <xsd:enumeration value="MC"/> <!-- Macerata  -->
            <xsd:enumeration value="ME"/> <!-- Messina  -->
            <xsd:enumeration value="MI"/> <!-- Milano  -->
            <xsd:enumeration value="MN"/> <!-- Mantova  -->
            <xsd:enumeration value="MO"/> <!-- Modena  -->
            <xsd:enumeration value="MS"/> <!-- Massa Carrara  -->
            <xsd:enumeration value="MT"/> <!-- Matera  -->
            <xsd:enumeration value="NA"/> <!-- Napoli  -->
            <xsd:enumeration value="NO"/> <!-- Novara  -->
            <xsd:enumeration value="NU"/> <!-- Nuoro  -->
            <xsd:enumeration value="OG"/> <!-- Ogliara  -->
            <xsd:enumeration value="OR"/> <!-- Oristano  -->
            <xsd:enumeration value="OT"/> <!-- Olbia-Tempio  -->
            <xsd:enumeration value="PA"/> <!-- Palermo  -->
            <xsd:enumeration value="PC"/> <!-- Piacenza  -->
            <xsd:enumeration value="PD"/> <!-- Padova  -->
            <xsd:enumeration value="PE"/> <!-- Pescara  -->
            <xsd:enumeration value="PG"/> <!-- Perugia  -->
            <xsd:enumeration value="PI"/> <!-- Pisa  -->
            <xsd:enumeration value="PN"/> <!-- Pordenone  -->
            <xsd:enumeration value="PO"/> <!-- Prato  -->
            <xsd:enumeration value="PR"/> <!-- Parma  -->
            <xsd:enumeration value="PT"/> <!-- Pistoia  -->
            <xsd:enumeration value="PU"/> <!-- Pesaro-Urbino  -->
            <xsd:enumeration value="PV"/> <!-- Pavia  -->
            <xsd:enumeration value="PZ"/> <!-- Potenza  -->
            <xsd:enumeration value="RA"/> <!-- Ravenna  -->
