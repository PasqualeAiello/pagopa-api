            <xsd:enumeration value="RI"/> <!-- Rieti  -->
            <xsd:enumeration value="RM"/> <!-- Roma  -->
            <xsd:enumeration value="RN"/> <!-- Rimini  -->
            <xsd:enumeration value="RO"/> <!-- Rovigo  -->
            <xsd:enumeration value="SA"/> <!-- Salerno  -->
            <xsd:enumeration value="SI"/> <!-- Siena  -->
            <xsd:enumeration value="SO"/> <!-- Sondrio  -->
            <xsd:enumeration value="SP"/> <!-- La Spezia  -->
            <xsd:enumeration value="SR"/> <!-- Siracusa  -->
            <xsd:enumeration value="SS"/> <!-- Sassari  -->
            <xsd:enumeration value="SU"/> <!-- Sud Sardegna  -->
            <xsd:enumeration value="SV"/> <!-- Savona  -->
